--
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>_Website Trials' Blog</title>
	<link href="http://hapaxia.github.io/_WebsiteTrials/blog/atom.xml" rel="self"/>
	<link href="http://hapaxia.github.io/_WebsiteTrials/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://hapaxia.github.io/_WebsiteTrials/blog</id>
	<author>
		<name>_WebsiteTrials</name>
		<email>?@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://hapaxia.github.io/_WebsiteTrials{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://hapaxia.github.io/_WebsiteTrials/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
